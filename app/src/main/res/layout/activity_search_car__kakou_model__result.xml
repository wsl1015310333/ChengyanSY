<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/activity_search_car__kakou_model__result"
    android:layout_width="match_parent"
android:background="#F5F5F5"
    android:layout_height="match_parent"
    >
<include layout="@layout/toobar"></include>
    <LinearLayout
        android:orientation="vertical"
        android:background="#FFFFFF"
        android:elevation="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
        <LinearLayout
            android:orientation="vertical"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="16dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14dp"
                    android:textColor="#353535"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车牌："
                    />
                <TextView
                    android:id="@+id/searchCar_Result_plate_numberr"
                    android:textSize="14dp"
                    android:textColor="#353535"
                    android:text="鲁BA248"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/kk_model"
                    android:textSize="14dp"
                    android:textColor="#353535"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="车型："
                    />
                <TextView
                    android:id="@+id/searchCar_Result_model"
                    android:textSize="14dp"
                    android:textColor="#353535"
                    android:text="大众-途观-2014；2015"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/searchCar_date"
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14dp"
                    android:textColor="#353535"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="查询时段："
                    />
                <TextView
                    android:id="@+id/searchCar_firstTime"
                    android:textSize="14dp"
                    android:textColor="#353535"
                    android:text="17-04-29 10:10:10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="14dp"
                    android:textColor="#353535"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" 至 "
                    />
                <TextView
                    android:id="@+id/searchCar_endTime"
                    android:textSize="14dp"
                    android:textColor="#353535"
                    android:text="17-04-29 10:10:10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="25dp">
                <TextView
                    android:textSize="14dp"
                    android:textColor="#353535"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="共 "
                    />
                <TextView
                    android:id="@+id/searchCar_result"
                    android:textSize="14dp"
                    android:textColor="#FE4D3D"
                    android:text="88"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textSize="14dp"
                    android:textColor="#353535"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" 条结果"
                    />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
<searchproject.php.yisa.chengyansy.view.RefreshRecyclerView
    android:elevation="2dp"
    android:id="@+id/searchcar_kakou_model_RecyclerView"
   android:layout_marginTop="9dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

</searchproject.php.yisa.chengyansy.view.RefreshRecyclerView>
</LinearLayout>
